               <children>
                  <Button fx:id="btnEnviar" mnemonicParsing="false" onAction="#sendComand" text="Enviar">
                     <HBox.margin>
                        <Insets left="190.0" right="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btnLimpar" mnemonicParsing="false" onAction="#limpar" text="Limpar" />
               </children>
            </HBox>
            <Label text="Dados Enviados:" />
            <TextArea fx:id="txtEnviado" prefHeight="200.0" prefWidth="200.0" />
            <Label text="Dados Recebidos:" />
            <TextArea fx:id="txtRecebido" prefHeight="200.0" prefWidth="200.0" />
         </children>
      </VBox>
   </children>
